{
  "arrowParens": "always",
  "endOfLine": "lf",
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "importOrder": [
    "@app/test-utils",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@ensdomains/(.*)$",
    "",
    "^@app/(.*)$",
    "",
    "^[./]"
  ],
  "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
